    "@types/chai": "^4.0.3",
    "@types/mocha": "^2.2.41",
    "chai": "^3.5.0",
    "mocha": "^3.5.0",
    "mocha-phantomjs-core": "^2.1.2",
    "ts-loader": "^3.2.0",
    "tslint": "^5.7.0",
    "typescript": "^2.0.0",
    "watch": "^1.0.2",
    "webix": ">=5.1.1",
    "webpack": "^3.5.4",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "node-polyglot": "^2.2.2",
    "webix-routie": "^0.4.0"
  },
