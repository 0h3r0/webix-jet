{
  "name": "webix-jet",
  "version": "1.5.1",
  "description": "Webix Jet microframework",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "webpack && phantomjs node_modules/mocha-phantomjs-core/mocha-phantomjs-core.js tests/index.html spec",
    "lint": "tslint --project tsconfig.json sources/**/*.ts",
    "dist": "tsc -p ./tsconfig.es5.json",
